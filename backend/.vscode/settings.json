{
    "flake8.args": [
        "--max-line-length=100"
    ],
    "autopep8.args": [
        "--max-line-length=100"
    ],
    "[python]": {
        "diffEditor.ignoreTrimWhitespace": false,
        "gitlens.codeLens.symbolScopes": [
            "!Module"
        ],
        "editor.formatOnType": true,
        "editor.wordBasedSuggestions": "off",
        "editor.formatOnSave": false,
    },
    "python.defaultInterpreterPath": ".venv/bin/python",
}